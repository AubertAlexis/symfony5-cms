{% extends "admin/base.html.twig" %}

{% block title "template.add.title"|trans %}

{% block title_h1 "template.add.h1"|trans %}

{% block body %}

	{{ form_start(form) }}
	
	<div class="row">

		<div class="col-12">
			<hr>
		</div>

		<div class="col-md-12 col-sm-12">
			<div class="card shadow mt-3">
				<div class="card-header d-flex align-items-center">
					<h6 class="m-0 font-weight-bold text-primary">{{ "form.title.content"|trans }}</h5>
				</div>
				<div class="card-body">
					
					{{ block("content", "admin/template/_form.html.twig") }}
					
				</div>
			</div>
		</div>

		{{ block("submit", "admin/template/_form.html.twig") }}

	</div>
	
	{{ form_end(form, {"render_rest": false}) }}

{% endblock %}

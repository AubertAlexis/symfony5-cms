{% extends "base.html.twig" %}

{% block body %}

	<div class="container">
		<div class="row">
			<div class="col-12 text-center">
				<h1>{{ page.title }}</h1>
			</div>
		</div>
		<div class="row">
			<div class="col-12">
				{% if template.imageName %}
					<img src="{{ vich_uploader_asset(template, "imageFile") }}" class="adminlist-image" alt="Image permis">
				{% endif %}
			</div>
			<div class="col-12">
				{{ template.content|raw }}
			</div>
		</div>
	</div>
	
{% endblock %}
